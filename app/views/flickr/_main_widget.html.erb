<div class="flickr-main">
	<ul>
		<% photos.each do |row| %>
			<li>
			<% row.each do |photo| %>
					<%= link_to(image_tag(FlickRaw.url_b(get_info(photo.id)), :class => 'flickr_photo', 
								:title => photo.title, :border => 0, :size => '75x75'), FlickRaw.url_photopage(get_info(photo.id)) ) %>
			<%end%>
			</li>
		<% end %>
	</ul>
</div>